<h2>Админка</h2>
<hr/>
<table class="table table-striped"> 
	<thead> 
		<tr> 
			<th>#</th> 
			<th>Name</th> 
			<th>Phone</th> 
			<th>Adress</th>
			<th>Order list</th> 
		</tr> 
	</thead> 
	<tbody>
		<% @o.each do |o| %> 
		<tr> 
			<th><%=o.id%></th> 
			<td><%=o.name%></td> 
			<td><%=o.phone%></td> 
			<td><%=o.adress%></td>
			<td><%=o.order_list%></td> 
		</tr>
		<%end%> 
	</tbody> 
</table>